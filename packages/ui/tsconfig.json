{
	"compilerOptions": {
		"incremental": true,
		"allowImportingTsExtensions": true,
		"jsx": "react",
		"jsxFactory": "window.RUMIOUS_JSX.createElement",
		"declaration": true,
		"declarationDir": "./dist/",
		"outDir": "./dist",
		"esModuleInterop": true,
		"strict": true,
		"target": "ES6",
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"lib": ["ES2017", "DOM"]
	},
	"include": ["./", "**/jsx.d.ts"],
	"exclude": ["node_modules"]
}